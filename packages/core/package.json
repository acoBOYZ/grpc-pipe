{
  "name": "@grpc-pipe/core",
  "version": "0.1.3",
  "author": "ACO .",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "grpc",
    "grpc communication",
    "grpc streaming",
    "pipe messaging",
    "typed communication",
    "type-safe rpc",
    "nodejs grpc",
    "bun grpc",
    "typescript grpc",
    "fast messaging",
    "binary protocol",
    "real-time messaging",
    "streaming rpc",
    "full duplex grpc",
    "custom transport protocol",
    "schema-based messaging",
    "protobuf transport",
    "grpc bidirectional stream",
    "typescript ipc",
    "efficient rpc"
  ],
  "engines": {
    "bun": ">=1",
    "node": ">=18"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./node_modules && rimraf ./coverage",
    "build": "bun run proto:compile && bun build ./src/index.ts --outdir dist --target node && bun run type:build && echo '[@grpc-pipe/core] build done âœ…'",
    "type:build": "tsc -p tsconfig.json",
    "proto:compile": "protoc --ts_proto_out=src/types --ts_proto_opt=outputServices=grpc-js --proto_path=proto proto/*.proto"
  },
  "devDependencies": {
    "@bufbuild/protobuf": "^2.2.5",
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.7.15",
    "@types/bun": "^1.2.9",
    "@types/node": "^22.14.1",
    "fastq": "^1.19.1",
    "rxjs": "^7.8.2",
    "typescript": "^5.8.3"
  }
}
