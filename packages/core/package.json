{
  "name": "@grpc-pipe/core",
  "version": "1.0.34",
  "author": "ACO .",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "grpc",
    "grpc communication",
    "grpc streaming",
    "pipe messaging",
    "typed communication",
    "type-safe rpc",
    "nodejs grpc",
    "bun grpc",
    "typescript grpc",
    "fast messaging",
    "binary protocol",
    "real-time messaging",
    "streaming rpc",
    "full duplex grpc",
    "custom transport protocol",
    "schema-based messaging",
    "protobuf transport",
    "grpc bidirectional stream",
    "typescript ipc",
    "efficient rpc"
  ],
  "engines": {
    "bun": ">=1",
    "node": ">=18"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./node_modules",
    "build": "bun run proto:compile && bun run clean && bun run build:esm && bun run build:types && echo '[@grpc-pipe/core] build done âœ…'",
    "build:esm": "tsc -p tsconfig.json",
    "build:types": "tsc -p tsconfig.types.json",
    "proto:compile": "protoc --ts_proto_out=src --ts_proto_opt=useOptionals=messages,outputServices=grpc-js,importSuffix=.js --proto_path=proto proto/*.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.3",
    "@grpc/grpc-js": "^1.13.4",
    "fastq": "^1.19.1"
  },
  "devDependencies": {}
}