{
  "name": "@grpc-pipe/core",
  "version": "0.1.67",
  "author": "ACO .",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "keywords": [
    "grpc",
    "grpc communication",
    "grpc streaming",
    "pipe messaging",
    "typed communication",
    "type-safe rpc",
    "nodejs grpc",
    "bun grpc",
    "typescript grpc",
    "fast messaging",
    "binary protocol",
    "real-time messaging",
    "streaming rpc",
    "full duplex grpc",
    "custom transport protocol",
    "schema-based messaging",
    "protobuf transport",
    "grpc bidirectional stream",
    "typescript ipc",
    "efficient rpc"
  ],
  "engines": {
    "bun": ">=1",
    "node": ">=18"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./node_modules",
    "build": "bun run proto:compile && bun run clean && bun run build:cjs && bun run build:esm && bun run build:types && echo '[@grpc-pipe/core] build done âœ…'",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && bun run rename:cjs",
    "rename:cjs": "find dist/cjs -name '*.js' -exec bash -c 'mv \"$0\" \"${0%.js}.cjs\"' {} \\;",
    "build:types": "tsc -p tsconfig.types.json",
    "proto:compile": "protoc --ts_proto_out=src/types --ts_proto_opt=outputServices=grpc-js --proto_path=proto proto/*.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.5.0",
    "@grpc/grpc-js": "^1.13.4",
    "fastq": "^1.19.1"
  },
  "devDependencies": {}
}