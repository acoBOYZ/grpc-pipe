  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": [
    "grpc",
    "grpc communication",
    "grpc streaming",
    "pipe messaging",
    "typed communication",
    "type-safe rpc",
    "nodejs grpc",
    "bun grpc",
    "typescript grpc",
    "fast messaging",
    "binary protocol",
    "real-time messaging",
    "streaming rpc",
    "full duplex grpc",
    "custom transport protocol",
    "schema-based messaging",
    "protobuf transport",
    "grpc bidirectional stream",
    "typescript ipc",
    "efficient rpc"
  ],
  "engines": {
    "bun": ">=1",
    "node": ">=18"
